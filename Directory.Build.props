<Project>
 <PropertyGroup>
    <LangVersion>9.0</LangVersion>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <AppendRuntimeIdentifierToOutputPath>false</AppendRuntimeIdentifierToOutputPath>
    <MSBuildProjectExtensionsPath>$(SolutionDir)\bin\intermediate\obj\$(MSBuildProjectName)</MSBuildProjectExtensionsPath>
    <BaseIntermediateOutputPath>$(SolutionDir)\bin\intermediate\obj</BaseIntermediateOutputPath>
    <OutputPath>$(SolutionDir)\bin\intermediate\bin\$(MSBuildProjectName)\$(Configuration)\</OutputPath>
 </PropertyGroup>
  <PropertyGroup>
    <Version>2.4</Version>
    <AssemblyVersion>2.4.65.3271</AssemblyVersion>
    <Authors>Caelan Sayler</Authors>
    <Product>Clowd</Product>
    <NeutralLanguage>en</NeutralLanguage>
    <Description>A screen/video capture and editing tool</Description>
    <Copyright>Copyright Â© Caelan Sayler, 2014-2021</Copyright>
    <PackageTags>screen capture, video, image editing</PackageTags>
    <RepositoryUrl>https://github.com/clowd</RepositoryUrl>
  </PropertyGroup>
</Project>